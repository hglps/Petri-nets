        <text>(* Q.4 -  Funcao que crie uma lista de pacotes para uma string e um tamanho de dados*)


fun createPckts(txt:string, len:int, count:int) =
    if count*len &lt; String.size(txt)
    then [(count+1, String.substring(txt, count*len, len))]^^(createPckts(txt, len, count+1))
    else [];
 
fun newPckts(&quot;&quot;:string, length:int) = []
| newPckts(text:string, 0) = []
| newPckts(text:string, length:int) =
createPckts(text, length, 0);   


newPckts(&quot;abcdef&quot;,2);
newPckts(&quot;&quot;,3);
newPckts(&quot;a&quot;,1);
newPckts(&quot;aaaaa&quot;,0);</text>
      </Aux>
