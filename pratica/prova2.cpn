        <label/>
        <text>(* Q.2 - Retorne sublista com pos1 e pos2 *)
(* Considere que as posicoes da lista L sao L[1], L[2], ... L[n] *)


fun sublists([],pos1,pos2) =  []
| sublists(l, pos1, pos2) =
    if pos1 &gt; pos2
    then []
    else if pos1 &gt; 1
    then sublists(tl l, pos1-1, pos2-1)
    else if pos1 = 1 andalso pos2 &gt; 1
    then [hd l]^^sublists(tl l, pos1, pos2-1)
    else [hd l];

sublists([10,20,30,40,50,60],2,4);
sublists([10,20],1,2);
sublists([10,20,30,40,50,60],4,2);</text>
      </Aux>
